@model List<AllCarViewModel>

@{
    ViewBag.Title = "My Cars";
}

<h2 class="text-center">@ViewBag.Title</h2>
<hr />

@if (!Model.Any())
{
    <h2 class="text-center">You do not have any cars yet!</h2>
}
else
{
    <div class="row">
        @foreach (var car in Model)
        {
            <partial name="_CarPartial" model="@car" />
        }
    </div>
}